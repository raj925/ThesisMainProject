	keywords = {Cognitive neuroscience, Computational psychiatry, Confidence, Decision making, Metacognition, Psychopathology},
	pages = {443--451},
	file = {Full Text:/Users/joshua/Zotero/storage/IG8IQUJP/Rouault et al. - 2018 - Psychiatric Symptom Dimensions Are Associated With.pdf:application/pdf;ScienceDirect Snapshot:/Users/joshua/Zotero/storage/CCK8HSJ6/S0006322318300295.html:text/html},
