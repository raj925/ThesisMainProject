	file = {Full Text:/Users/joshua/Zotero/storage/7SELK2ET/Price and Stone - 2004 - Intuitive evaluation of likelihood judgment produc.pdf:application/pdf;Snapshot:/Users/joshua/Zotero/storage/4FVWL7MZ/bdm.html:text/html},
