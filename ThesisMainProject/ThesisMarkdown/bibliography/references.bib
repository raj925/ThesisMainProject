@article{bhise_defining_2018-1,
	title = {Defining and {Measuring} {Diagnostic} {Uncertainty} in {Medicine}: {A} {Systematic} {Review}},
	volume = {33},
	issn = {1525-1497},
	shorttitle = {Defining and {Measuring} {Diagnostic} {Uncertainty} in {Medicine}},
	url = {https://doi.org/10.1007/s11606-017-4164-1},
	doi = {10.1007/s11606-017-4164-1},
	abstract = {Physicians routinely encounter diagnostic uncertainty in practice. Despite its impact on health care utilization, costs and error, measurement of diagnostic uncertainty is poorly understood. We conducted a systematic review to describe how diagnostic uncertainty is defined and measured in medical practice.},
