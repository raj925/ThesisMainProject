@article{ais_individual_2016-2,
	title = {Individual consistency in the accuracy and distribution of confidence judgments},
	volume = {146},
	issn = {0010-0277},
	url = {https://www.sciencedirect.com/science/article/pii/S0010027715300846},
	doi = {10.1016/j.cognition.2015.10.006},
	abstract = {We examine which aspects of the confidence distributions – its shape, its bias toward higher or lower values, and its ability to distinguish correct from erred trials – are idiosyncratic of the who (individual specificity), the when (variability across days) and the what (task specificity). Measuring confidence across different sessions of four different perceptual tasks we show that: (1) Confidence distributions are virtually identical when measured in different days for the same subject and the same task, constituting a subjective fingerprint, (2) The capacity of confidence reports to distinguish correct from incorrect responses is only modestly (but significantly) correlated when compared across tasks, (3) Confidence distributions are very similar for tasks that involve different sensory modalities but have similar structure, (4) Confidence accuracy is independent of the mean and width of the confidence distribution, (5) The mean of the confidence distribution (an individual’s confidence bias) constitutes the most efficient indicator to infer a subject’s identity from confidence reports and (6) Confidence bias measured in simple perceptual decisions correlates with an individual’s optimism bias measured with standard questionnaire.},
	urldate = {2024-10-07},
	journal = {Cognition},
	author = {Ais, Joaquín and Zylberberg, Ariel and Barttfeld, Pablo and Sigman, Mariano},
	month = jan,
	year = {2016},
	keywords = {Metacognitive judgments, Confidence, Perceptual decisions, Psychophysics},
	pages = {377--386},
}

@article{vickers_effects_1982-2,
	title = {Effects of alternating set for speed or accuracy on response time, accuracy and confidence in a unidimensional discrimination task},
	volume = {50},
	issn = {0001-6918},
	url = {https://www.sciencedirect.com/science/article/pii/0001691882900063},
	doi = {10.1016/0001-6918(82)90006-3},
	abstract = {Three approaches to a theoretical analysis of confidence judgments are considered: one linking confidence to the number of sensory observations, one based on a distinction between ‘state’ and ‘process’ factors, and a ‘balance of evidence’ hypothesis developed from an accumulator model of discrimination. An experiment is described in which observers were asked to decide which of two parallel lines was the longer, and to rate their confidence in each judgment. Each observer's set for speed or accuracy was manipulated over successive blocks of trials, and effects on response time, accuracy, and confidence were examined. Under an accuracy set, observers produced a higher percentage of correct responses, had longer response times, and made more confident judgments than under a set for speed. Within both speed and accuracy blocks, however, confidence ratings were inversely related to response time. The data on response accuracy, time, and confidence indicate certain deficiencies in either of the first two approaches, but were well accounted for by the third.},
	number = {2},
	urldate = {2024-10-07},
	journal = {Acta Psychologica},
	author = {Vickers, Douglas and Packer, Jeanette},
	month = mar,
	year = {1982},
	note = {Number: 2},
	pages = {179--197},
}

@article{charles_dynamic_2019,
	title = {Dynamic sources of evidence supporting confidence judgments and error detection},
	volume = {45},
	issn = {1939-1277},
	doi = {10.1037/xhp0000583},
	abstract = {Our decisions are accompanied by a subjective sense of confidence about whether the choices we have made are correct or erroneous. We investigate the information on which these confidence judgments are based, and how they relate to the decision itself, by studying how fluctuations in perceptual information influence decisions and second-order metacognitive evaluations of confidence and accuracy. Human participants judged which of two dynamically changing stimuli contained more dots, under instructions emphasizing either speed or accuracy. Crucially, stimuli remained visible for one second after the decision, before participants rated their confidence in their choice. We found that confidence and error detection depended on the balance of stimulus evidence accumulated in the periods both preceding and following the initial decision, regardless of whether instructions emphasized speed or accuracy. These findings suggest a shared computational basis for error detection and confidence judgments, with implications for current models of metacognitive evaluation of decision processes. (PsycInfo Database Record (c) 2020 APA, all rights reserved)},
	number = {1},
	journal = {Journal of Experimental Psychology: Human Perception and Performance},
	author = {Charles, Lucie and Yeung, Nick},
	year = {2019},
	note = {Number: 1
Place: US
Publisher: American Psychological Association},
	keywords = {Decision Making, Metacognition, Judgment, Self-Confidence, Choice Behavior},
	pages = {39--52},
}

@article{boldt_shared_2015,
	title = {Shared {Neural} {Markers} of {Decision} {Confidence} and {Error} {Detection}},
	volume = {35},
	copyright = {Copyright © 2015 Boldt and Yeung. This article is freely available online through the J Neurosci Author Open Choice option.},
	issn = {0270-6474, 1529-2401},
	url = {https://www.jneurosci.org/content/35/8/3478},
	doi = {10.1523/JNEUROSCI.0797-14.2015},
	abstract = {Empirical evidence indicates that people can provide accurate evaluations of their own thoughts and actions by means of both error detection and confidence judgments. This study investigates the foundations of these metacognitive abilities, specifically focusing on the relationship between confidence and error judgments in human perceptual decision making. Electroencephalography studies have identified the error positivity (Pe)—an event-related component observed following incorrect choices—as a robust neural index of participants' awareness of their errors in simple decision tasks. Here we assessed whether the Pe also varies in a graded way with participants' subjective ratings of decision confidence, as expressed on a 6-point scale after each trial of a dot count perceptual decision task. We observed clear, graded modulation of the Pe by confidence, with monotonic reduction in Pe amplitude associated with increasing confidence in the preceding choice. This effect was independent of objective accuracy. Multivariate decoding analyses indicated that neural markers of error detection were predictive of varying levels of confidence in correct decisions, including subtle shifts in high-confidence trials. These results suggest that shared mechanisms underlie two forms of metacognitive evaluation that are often treated separately, with consequent implications for current theories of their neurocognitive basis.},
	language = {en},
	number = {8},
	urldate = {2024-10-07},
	journal = {Journal of Neuroscience},
	author = {Boldt, Annika and Yeung, Nick},
	month = feb,
	year = {2015},
	pmid = {25716847},
	note = {Number: 8
Publisher: Society for Neuroscience
Section: Brief Communications},
	keywords = {decision making, confidence, cognitive control, error monitoring, error positivity, metacognition},
	pages = {3478--3484},
}

@misc{xue_challenging_2023,
	title = {Challenging the {Bayesian} confidence hypothesis},
	url = {https://osf.io/mf5zp},
	doi = {10.31234/osf.io/mf5zp},
	abstract = {The Bayesian confidence hypothesis, which postulates that confidence reflects the posterior probability that a decision is correct, is currently the most prominent theory of confidence. Although several recent studies have found evidence against a strictly Bayesian account in the context of relatively complex tasks, the Bayesian confidence hypothesis remains dominant for simpler tasks. Here, we show that there is a basic behavioral signature that distinguishes Bayesian from distance-to-criterion models even for simple 2-choice tasks. We examined this signature in two experiments and found that both experiments exhibited behavioral signatures in line with distance-to-criterion computations but contrary to Bayesian ones. We further performed an extensive comparison of 32 models that implemented either Bayesian or distance-to-criterion confidence computations and systematically differed in their auxiliary assumptions. These model comparisons provided overwhelming support for the distance-to-criterion models over their Bayesian counterparts across all model variants and across both experiments. These observations challenge the Bayesian confidence hypothesis and instead suggest that humans use simple distance-to-criterion computations when judging confidence.},
	language = {en-us},
	urldate = {2024-10-07},
	publisher = {OSF},
	author = {Xue, Kai and Shekhar, Medha and Rahnev, Dobromir},
	month = sep,
	year = {2023},
	keywords = {Confidence, perceptual decision making, Signal detection theory, visual metacognition},
}

