Publisher: SAGE Publications Inc STM},
	pages = {31--44},
}

@article{arocha_hypothesis_1993,
	title = {Hypothesis {Generation} and the {Coordination} of {Theory} and {Evidence} in {Novice} {Diagnostic} {Reasoning}},
