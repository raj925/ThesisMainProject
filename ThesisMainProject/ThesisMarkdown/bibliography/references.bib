@article{payne_effect_2012,
	series = {Dissertation {Abstracts} {International}},
	title = {Effect of a metacognitive intervention on cognitive heuristic use during diagnostic reasoning.},
	volume = {72},
	issn = {["0419-4217", "978-1-124-83380-4"]},
	abstract = {Medical judgment and decision-making frequently occur under conditions of uncertainty. In order to reduce the complexity of diagnosis, physicians often rely on cognitive heuristics. Use of heuristics during clinical reasoning can be effective; however when used inappropriately the result can be flawed reasoning, medical errors and patient harm. Many researchers have attempted to debias individuals from inappropriate heuristic use by designing interventions based on normative theories of decision-making. There have been few attempts to debias individuals using interventions based on descriptive decision-making theories. Objectives: (1) Assess use of Anchoring and Adjustment and Confirmation Bias during diagnostic reasoning; (2) Investigate the impact of heuristic use on diagnostic accuracy; (3) Determine the impact of a metacognitive intervention based on the Mental Model Theory designed to reduce biased judgment by inducing physicians to 'think about how they think'; and (4) Test a novel technique using eye-tracking to determine heuristic use and diagnostic accuracy within mode of thinking as defined by the Dual Process Theory. Methods: Medical students and residents assessed clinical scenarios using a computer system, specified a diagnosis, and designated the data used to arrive at the diagnosis. During case analysis, subjects either verbalized their thoughts or wore eye-tracking equipment to capture eye movements and pupil size as they diagnosed cases. Diagnostic data specified by the subject was used to measure heuristic use and assess the impact of heuristic use on diagnostic accuracy. Eye-tracking data was used to determine the frequency of heuristic use (Confirmation Bias only) and mode of thinking. Statistic models were executed to determine the effect of the metacognitive intervention. Results: Use of cognitive heuristics during diagnostic reasoning was common for this subject population. Logistic regression showed case difficulty to be an important factor contributing to diagnostic error. The metacognitive intervention had no effect on heuristic use and diagnostic accuracy. Eye-tracking data reveal this subject population infrequently assess cases in the Intuitive mode of thinking; spend more time in the Analytical mode of thinking, and switches between the two modes frequently as they reason through a case to arrive at a diagnosis. (PsycInfo Database Record (c) 2021 APA, all rights reserved)},
	language = {English     PT  - Dissertation Abstract},
	number = {11},
	journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
	author = {Payne, Velma Lucille},
	year = {2012},
	note = {Place: US
Publisher: ["ProQuest Information \& Learning", "US"]},
	keywords = {*Diagnosis, *Dual Process Models, *Intervention, *Medical Students, *Reasoning, Cognition, Heuristics, Metacognition, Visual Tracking},
	pages = {7060},
	annote = {RAYYAN-INCLUSION: \{"oxstar@nda.ox.ac.uk"={\textgreater}"Included"\}},
	file = {Payne - 2012 - Effect of a metacognitive intervention on cognitiv.pdf:/Users/srirajaiyer/Zotero/storage/8ITT5CKC/Payne - 2012 - Effect of a metacognitive intervention on cognitiv.pdf:application/pdf},
}

@article{woods_role_2007,
	series = {Dissertation {Abstracts} {International}},
	title = {The role of biomedical knowledge in medical diagnosis by learners.},
	volume = {67},
	issn = {0419-4217},
	abstract = {Medical students must master several kinds of medical knowledge. In addition to knowledge of signs and symptoms, an understanding of the probabilities associated with disease and the likelihood of specific patterns of clinical findings, physicians must also have biomedical knowledge of underlying anatomy, physiology and biological mechanisms. The specific role of each form of knowledge and the relevance of certain types of knowledge to diagnostic decision-making and medical education is often debated. While there is general agreement that the acquisition of clinical knowledge is a key component of medical training, research into the value of biomedical knowledge has yielded little evidence supporting its value as an aid to diagnosis. I examined the role of biomedical knowledge through a series of experiments that required novice participants to learn a set of disease categories using either biomedical mechanisms or clinical knowledge as learning tools. The studies presented in this thesis provide evidence that knowledge of biomedical mechanisms can be used to identify the features of a disease category. For the novice diagnostician, the integration of features allows for a more coherent mental representation that is more memorable than the disease probabilities created by more quantitative decision aids. The results of several experiments demonstrate that students with an integrated understanding of mechanisms are better able to maintain diagnostic performance over time. This effect extended to very simple causal theories used to integrate the signs and symptoms of a disease category. When compared to a comprehensive list of features, knowledge of these simple theoretical connections led to an improvement in diagnostic performance over time. Consistent with the medical education literature, I have found that biomedical knowledge is not always necessary. Participants taught only the clinical features of disease initially achieved a level of diagnostic performance comparable to that attained by students with knowledge of underlying mechanisms. However this high level of diagnostic performance apparently occurs because knowledge of underlying mechanisms is not used immediately after learning. After a delay participants undergo a switch in processing and begin to make use of their knowledge of underlying mechanisms. In addition to delayed testing, I have found that students will make use of their knowledge of underlying mechanisms when challenged with difficult cases or with novel terminology. (PsycInfo Database Record (c) 2021 APA, all rights reserved)},
	language = {English     PT  - Dissertation Abstract},
	number = {12},
	journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
	author = {Woods, Nicole Natasha},
	year = {2007},
	note = {Place: US
Publisher: ["ProQuest Information \& Learning", "US"]},
	keywords = {*Diagnosis, *Decision Making, *Physicians, Medical Education},
	pages = {7401},
	annote = {RAYYAN-INCLUSION: \{"oxstar@nda.ox.ac.uk"={\textgreater}"Included"\}},
	file = {Woods - 2007 - The role of biomedical knowledge in medical diagno.pdf:/Users/srirajaiyer/Zotero/storage/B6BSJU72/Woods - 2007 - The role of biomedical knowledge in medical diagno.pdf:application/pdf},
}

@article{heller_heuristics_1992,
	series = {The {Quarterly} {Journal} of {Experimental} {Psychology}, {The} {Quarterly} {Journal} of {Experimental} {Psychology} {B}: {Comparative} and {Physiological} {Psychology}},
	title = {Heuristics in medical and non-medical decision-making.},
	volume = {44},
	issn = {0272-4987},
	doi = {10.1080/02724989243000019},
	abstract = {Presented pairs of hypothetical medical and nonmedical problems to 44 pediatric residents at 3 levels of hospital training. Each problem was designed to detect a specific heuristic-based bias in making diagnoses. A greater number of 3rd-yr Ss disregarded base-rate information than did 1st- and 2nd-yr Ss on medical but not on nonmedical problems. On medical problems, a greater number of 1st-yr Ss expressed greater confidence in redundant information that was contextually embedded than in information that was presented in a listed format. Over one-third of the Ss confused prospective and retrospective probabilities; three-fourths showed evidence of augmentation; virtually all Ss expressed greater confidence in a diagnosis based on redundant rather than on nonredundant listed information. (PsycInfo Database Record (c) 2021 APA, all rights reserved)},
	language = {English     PT  - Journal, Peer Reviewed Journal},
	number = {2},
	journal = {The Quarterly Journal of Experimental Psychology A: Human Experimental Psychology},
	author = {Heller, Rachael F and Saltzstein, Herbert D and Caspe, William B},
	year = {1992},
	note = {Place: United Kingdom
Publisher: ["Taylor \& Francis", "United Kingdom"]},
	keywords = {*Medical Diagnosis, Decision Making, Heuristics, *Decision Making, *Physicians, *Cognitive Processes, *Medical Residency},
