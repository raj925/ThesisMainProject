	keywords = {Reasoning, Expertise, Thinking, Psychology, Chess, Human experimentation, Memory, Pattern recognition, Problem solving, Problem-solving strategies, Specialization},
	pages = {1117--1143},
}

@article{hautz_diagnostic_2019-2,
